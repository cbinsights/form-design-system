{
  "name": "@cbinsights/fds",
  "description": "Form: A design system by CB Insights",
  "author": "CB Insights",
  "version": "0.0.0-development",
  "license": "MIT",
  "files": [
    "lib"
  ],
  "scripts": {
    "codeshift": "/bin/sh ./scripts/bash/transforms/transform.sh",
    "clean": "/bin/sh ./scripts/bash/clean.sh",
    "build:bootstrap": "/bin/sh ./scripts/bash/bootstrapBuild.sh",
    "build:styles": "yarn dictionary:css & yarn styles:min && yarn styles:full",
    "build:icons": "yarn icons:png && yarn icons:svg && yarn icons:css",
    "build:comp": "yarn comp:css && yarn comp:jsx",
    "build:distCSS": "NODE_ENV=production postcss src/assets/all-styles.css -o ./lib/assets/all-styles.min.css --config=./postcss.config.js",
    "build:full": "yarn clean && yarn build:bootstrap && yarn build:styles && yarn build:icons && yarn build:comp && yarn build:distCSS && yarn build:types && yarn build:images",
    "build:images": "cp -r src/assets/images/ lib/assets/images/",
    "build:types": "tsc  --noEmit false --emitDeclarationOnly true --declaration true --outDir lib",
    "build": "yarn build:full",
    "gen:dictionary": "node ./scripts/node/dictionary/build.js",
    "gen:icons": "yarn icons:react && yarn icons:setup",
    "styles:min": "NODE_ENV=production postcss src/base-styles/index.css --verbose --output=lib/base-styles/base-styles.min.css --config=./postcss.config.js",
    "styles:full": "postcss src/base-styles/index.css --verbose --output=lib/base-styles/base-styles.full.css --config=./postcss.config.js",
    "styles:docs": "NODE_ENV=base_styles_docs postcss ./lib/base-styles/base-styles.full.css --output=/dev/null",
    "icons:export": "node ./scripts/node/icons/exportFromSketch.js",
    "icons:svg": "node ./scripts/node/icons/buildSvg.js",
    "icons:png": "node ./scripts/node/icons/buildPng.js",
    "icons:react": "node ./scripts/node/icons/buildComponents.js",
    "icons:css": "cp src/icons/style/icons.css ./lib/icons/style/icons.css",
    "icons:setup": "node ./scripts/node/icons/buildIconManifest.js",
    "comp:jsx": "NODE_ENV=jsx_compile babel src --out-dir=lib --no-copy-ignored --extensions '.js,.jsx,.ts,.tsx'",
    "comp:css": "postcss src/components/style/index.css -o ./lib/components/style/fds-components.css --config=./postcss.config.js",
    "comp:docs": "build-storybook -o public",
    "dictionary:css": "cp -r src/dictionary/css lib/dictionary/",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint --max-warnings 0 ./src/ --ext .js,.jsx,.ts,.tsx",
    "lint:stylelint": "stylelint 'src/**/*.css'",
    "prettier:check": "prettier --check .",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "stats:css": "node ./scripts/node/util/getClassStats.js",
    "stats:imports": "node ./scripts/node/util/getImportStats.js",
    "type-check": "tsc",
    "semantic-release": "semantic-release",
    "commit": "git-cz",
    "check": "yarn lint && yarn test && yarn type-check",
    "postinstall": "yarn husky install",
    "prepublishOnly": "yarn pinst --disable",
    "postpublish": "yarn pinst --enable",
    "start:storybook": "start-storybook -p 9009 -s public --ci",
    "cy:test:storybook": "cypress run --headless -C cypress.storybook.json",
    "ci:start-and-test-storybook": "cross-env START_SERVER_AND_TEST_INSECURE=1 yarn start-server-and-test start:storybook http://localhost:9009 cy:test:storybook",
    "test-pattern": "yarn test -t"
  },
