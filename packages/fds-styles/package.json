{
  "name": "fds-styles",
  "version": "1.16.0",
  "description": "Style foundation for Form Design System",
  "author": "CB Insights",
  "license": "MIT",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "yarn precompile && yarn postcss:min && yarn postcss:full && yarn docs",
    "precompile": "mkdir -p ./lib",
    "postcss:min": "NODE_ENV=production postcss src/index.css --output=lib/fds-styles.css --config=./postcss.config.js",
    "postcss:full": "postcss src/index.css --output=lib/fds-styles.full.css --config=./postcss.config.js",
    "docs": "node ./buildDocs.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cbinsights/form-design-system.git"
  },
  "gitHead": "cc2e1975aa791bb367b6dd22d9bbd8dfa1db8bcb",
  "dependencies": {
    "fds-dictionary": "^1.16.0"
  }
}
